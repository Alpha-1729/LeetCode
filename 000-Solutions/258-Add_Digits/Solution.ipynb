   "source": [
    "# Approach\n",
    "- Every number which is divisible by 9 have sum of digits as 9;"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "class Solution {\n",
    "\n",
    "  public int addDigits(int num) {\n",
    "    if (num % 9 == 0 && num != 0) {\n",
    "      return 9;\n",
    "    } else {\n",
    "      return num % 9;\n",
    "    }\n",
    "  }\n",
    "}\n"
   ]
  },
